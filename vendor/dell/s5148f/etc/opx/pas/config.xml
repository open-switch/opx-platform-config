<pas>
        <subcat id="chassis"/>
        <subcat id="entity"/>
        <subcat id="psu"/>
        <subcat id="fan-tray"/>
        <subcat id="card"/>
        <subcat id="fan"/>
        <subcat id="led" entity-type="card"/>
        <subcat id="display" entity-type="card"/>
